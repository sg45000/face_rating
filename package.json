{
  "name": "wedding",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev:watch": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:gen_type": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name createPostImagesTB",
    "ngrok:start": "./ngrok http 3000"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.5",
    "@google-cloud/vision": "^2.3.2",
    "@line/bot-sdk": "^7.3.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/platform-express": "^7.6.17",
    "@prisma/client": "^2.23.0",
    "@types/sharp": "^0.28.1",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "encoding-japanese": "^1.0.30",
    "file-type": "^16.4.0",
    "iconv-lite": "^0.6.3",
    "prisma": "^2.23.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^7.1.0",
    "sharp": "^0.28.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@types/express": "^4.17.2",
    "@types/jest": "26.0.23",
    "@types/node": "^15.6.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.6",
    "ts-loader": "^9.2.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
